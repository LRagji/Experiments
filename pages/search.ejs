<!doctype html>
<html lang="en">

<head>
    <% include ./head %>

    <title>Health Mall-Search</title>
</head>

<body class="bg-light">
    <!-- Main Nav -->
    <% include ./header %>

    <!-- Site Content -->
    <div class="container">
        <div class="row">
            <div class="col">
                <Strong>
                    Searching for: "<span id="keyword">
                        <%-pageData.sk-%></span>"
                </strong>
            </div>
        </div>
        <div class="row">
            <div class="col">
                <div id="resultContainer">

                </div>

                <div id="loadingSpinner" class="my-2">
                    <div class="progress">
                        <div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar"
                            aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
                            <span id="loadingMessage">Loading Users</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Footer -->
    <% include ./footer %>
    <script src="../static/resources/scripts/paginatedsearch.js"></script>
</body>

</html>